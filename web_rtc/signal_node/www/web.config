<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="NodeInspector" patternSyntax="ECMAScript" stopProcessing="true">
          <match url="^app.js\/debug[\/]?"/>
        </rule>
        <rule name="StaticContent">
          <action type="Rewrite" url="{REQUEST_URI}"/>
        </rule>
        <rule name="DynamicContent">
          <conditions>
            <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="True"/>
          </conditions>
          <action type="Rewrite" url="app.js"/>
        </rule>
      </rules>
    </rewrite>
    <httpErrors>
      <remove statusCode="500"/>
      <error statusCode="500" path="C:\Inetpub\vhosts\immanuel-93753.package\error_docs\internal_server_error.html"/>
    </httpErrors>
    <tracing>
      <traceFailedRequests>
        <add path="*">
          <traceAreas>
            <add provider="WWW Server" areas="iisnode" verbosity="Verbose"/>
          </traceAreas>
          <failureDefinitions statusCodes="200-999"/>
        </add>
      </traceFailedRequests>
    </tracing>
    <!-- <iisnode devErrorsEnabled="false" loggingEnabled="true" logDirectory="C:\inetpub\logs\iisnode"/> -->
  </system.webServer>
  <system.web>
    <compilation tempDirectory="C:\Inetpub\vhosts\immanuel-93753.package\tmp"/>
  </system.web>
</configuration>
